{
  "mcpServers": {
    "elasticsearch": {
      "command": "npx",
      "args": [
        "mcp-remote",
        "${KIBANA_URL}/api/agent_builder/mcp",
        "--header",
        "Authorization:${ELASTICSEARCH_AUTH_HEADER}"
      ],
      "env": {
        "KIBANA_URL": "",
        "ELASTICSEARCH_AUTH_HEADER": ""
      }
    }
  }
}
